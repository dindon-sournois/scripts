#!/bin/bash

if [[ "$(hostname -s)" == "msi" ]]; then
    if [ $(uname -r | grep vfio -c) -eq 1 ]; then
        lxpolkit&
        synergy&
    fi
fi

if [[ "$(hostname -s)" == "x1c" ]]; then
    nm-applet&
fi

# compton -c -b
# i3-subscribe
xcmenu -daemon&
emacs --daemon
mpd
dunst&
firefox&
thunderbird&
termite -e ncmpcpp --name ncmpcpp&
i3-focus-last&
owncloud&
